Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! black
URxvt.color0:        #1C1C1C
URxvt.color8:        #666666
! red
URxvt.color1:        #ff005b
URxvt.color9:        #ff00a0
! green
URxvt.color2:        #cee318
URxvt.color10:       #ccff00
! yellow
URxvt.color3:        #ffe755
URxvt.color11:       #ff9f00
! blue
URxvt.color4  : #34346565A4A4
URxvt.color12 : #72729F9FCFCF
! magenta
URxvt.color5:        #833c9f
URxvt.color13:       #be67e1
! cyan 
URxvt.color6:        #0ac1cd
URxvt.color14:       #63e7f0
! white
URxvt.color7:        #e5e5e5
URxvt.color15:       #f3f3f3

URxvt*foreground:White

! fonts
! run "fc-list" for a list of available fonts
URxvt*font: xft:symon:Regular:pixelsize=16
!URxvt.font:            -*-symon-medium-r-*-*-16-*-*-*-*-*-*-r
URxvt*boldFont:

URxvt*scrollBar: False
URxvt*scrollTtyOutput: False
URxvt*scrollTtyKeypress: True
URxvt*secondaryScroll: True
URxvt*saveLines: 8000

URxvt.perl-ext-common : default,matcher,clipboard
URxvt.urlLauncher     : firefox
URxvt.matcher.button  : 1

URxvt*background:[90]Black
URxvt.depth: 32
URxvt*letterSpace: -0.5
URxvt*termName: xterm-256color
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.iso14755: false
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.Shift-Control-C: perl:clipboard:copy

